  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "node start-expo-ip-forcado.js",
    "start:tunnel": "node start-expo-ip-forcado.js --tunnel",
    "start:lan": "node start-expo-ip-forcado.js",
    "start:no-localhost": "node start-expo-forced-ip-no-localhost.js --lan",
    "start:no-localhost:tunnel": "node start-expo-forced-ip-no-localhost.js --tunnel",
    "start:qrcode-correto": "node start-expo-com-qrcode-correto.js --lan",
