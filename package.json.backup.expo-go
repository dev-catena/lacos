    "start:android": "node start-expo-android-fixed.js",
    "start:ios": "node start-expo-ios-fixed.js --clear --lan",
    "start:ios:tunnel": "node start-expo-ios-fixed.js --clear --tunnel",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "web:ip": "node scripts/start-web-ip-corrigido.js",
    "logs:android": "adb logcat | grep -i 'reactnative\\|expo' || echo 'Instale adb: sudo apt install android-tools-adb'",
    "logs:all": "adb logcat || echo 'Instale adb: sudo apt install android-tools-adb'",
    "debugger": "echo 'Pressione j no terminal do Expo para abrir DevTools, ou acesse: http://localhost:19000/debugger-ui/'"
  },
